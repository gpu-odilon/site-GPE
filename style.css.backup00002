/* Header style ---- Custom select*/
select {
    /* style */
    display: none !important;
    background-color: rgba(255, 255, 255, 0);
    background-image: url('../images/drapeau-fr.png');
    background-repeat: no-repeat;
    background-position: 35px 4px;
    border: solid #fddc00;
    color: #fddc00;
    border-radius: 15px;
    border-width: 0.17em;
    display: inline-block;
    font: inherit;
    line-height: 1em;
    padding: 0em 0.1em 0.2em 3.5em;
    font-weight: bold;
  
    /* reset */
    margin: 0px 0px 0px 0px;      
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -moz-appearance: none;
  }

.mobile-btn {
    display: none;
}

.mobile-close-btn {
    display: none;
}

.mobile-btn a{
    color: #f7d30b;
    margin: 30px;
    font-size: 30px;
}

.mobile-btn a:hover{
    color: #8234cf;
}


@media screen and (max-width: 1023px) {
    .menu-navigation {
        display: none;
    }
    .mobile-btn {
        display: block;
        float: right;
    }
    .mobile-close-btn {
        display: block;
        position: absolute;
        top: 0;
        right: 40px;
        font-size: 60px;
    }
    .mobile-close-btn a{
        color: #f7d30b !important;
    }
    .mobile-close-btn a:hover{
        color: #8234cf !important;
    }

    img.logo {
        background-repeat: none;
        max-width: auto;
        height: 25px;
    }
  }

  @media screen and (max-width: 1023px) {
    .menu-navigation.responsive{
        position: fixed;
        display: block;
        height: 100%;
        width: 100%;
        top: 0;
        left: 0;
        background-color: #310681f8;
        z-index: 1;
        overflow-x: hidden;
        transition: 0.5s;
    }
    .menu-navigation .responsive {
        position: relative;
        width: 100%;
    }
    .menu-navigation.responsive li {
        float: none;
        display: block;
        position: relative;
        text-align: center;
        top: 25%
    }
    .menu-navigation.responsive li a{
        font-size: 30px;
    }
    a.active-resp {
        text-align: center;
        margin: 0;
        padding: 0;
        background-color: #f7d30b;
        color: #000;
        display: block;
        border-radius: 20px;
    }
  }

