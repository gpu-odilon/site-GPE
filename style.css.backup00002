/*Footer Modal*/
.modal {
    display: none; /* Hidden by default */
    position: fixed; /* Stay in place */
    z-index: 99999; /* Sit on top */
    left: 0;
    top: 0;
    width: 100%; /* Full width */
    height: 100%; /* Full height */
    overflow: auto; /* Enable scroll if needed */
    background-color: rgb(0,0,0); /* Fallback color */
    background-color: rgba(0, 0, 0, 0.479); /* Black w/ opacity */
  }

  .modal-content {
    margin: 10% auto; /* 15% from the top and centered */
    padding: 20px;
    max-width: 100% /* Could be more or less, depending on screen size */
  }

  /* The Close Button */
.close-container {
    display: relative;
    display: block;
    text-align: right;
    max-width: 70%;
    margin: 0 auto;
  }

  @media screen and (max-width:769px) {
    .close-container {
        display: relative;
        display: block;
        text-align: right;
        max-width: 100%;
        margin: 0 auto;
      }
  }

.close-modal {
    color: #f7d30b;
    font-size: 60px;
    font-weight: bold;
  }
  
  .close-modal:hover,
  .close-modal:focus{
    color: #8234cf; 
    text-decoration: none;
    cursor: pointer;
  }

