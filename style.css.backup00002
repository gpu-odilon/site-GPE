/*Service carousel*/
.service-carousel-container {
    width: 900px;
    margin: 20px auto;
    height: 200px;
    position: relative;
}

@media screen and (max-width: 768px){
    .service-carousel-container {
        width: 300px;
    }
}

@media screen and (max-width: 1024px) and (min-width: 769px){
    .service-carousel-container {
        width: 600px;
    }
}

.service-carousel-container .service-carousel-inner{
    overflow: hidden;
}

.service-carousel-container .track {
    display: inline-flex;
    transition: transform 0.5s;
}

.service-carousel-container .service-card-container {
    min-width: 300px;
    flex-shrink: 0;
    height: 200px;
    padding: 0px 30px;
    box-sizing: border-box;
}

.service-carousel-container .service-card-container .service-card {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
}

.service-carousel-nav button{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    cursor: pointer;
}

button.service-carousel-prev {
    background-image: url('../images/button-left.png');
    background-color: rgba(255, 255, 255, 0);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
    width: 50px;
    height: 50px;
    border: 0;
    margin: 0;
    padding: 0;
}

button.service-carousel-next {
    background-image: url('../images/button-right.png');
    background-color: rgba(255, 255, 255, 0);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center;
    width: 50px;
    height: 50px;
    border: 0;
    margin: 0;
    padding: 0;
}

.service-carousel-nav .service-carousel-prev {
    left: -25px;
    display: none;
}

.service-carousel-nav .service-carousel-prev.show {
    display: block;
}

.service-carousel-nav .service-carousel-next {
    right: -25px;
}

.service-carousel-nav .service-carousel-next.hide {
    display: none;
}

.service-card > * {
    flex: 1;
}

.service-card .card-img {
    display: flex;
    justify-content: center;
    align-items: center;
}

.service-card .card-title {
    display: flex;
    justify-content: center;
    align-items: center;
}

